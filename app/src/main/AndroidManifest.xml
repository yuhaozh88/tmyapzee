<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="nkser.tmpayzee">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".GlobalValues">
        <activity android:name="nkser.tmpayzee.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="nkser.tmpayzee.GameActivity"
                  android:parentActivityName="nkser.tmpayzee.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="nkser.tmpayzee.MainActivity"/>
        </activity>
        <activity
            android:name="nkser.tmpayzee.SettingsActivity"
            android:label="@string/title_activity_settings">
        </activity>

    </application>

</manifest>